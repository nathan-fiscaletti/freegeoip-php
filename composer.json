{
    "name": "nafisc/ipstackgeo-php",
    "description": "A PHP library for interfacing with IPStack Geo API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Fiscaletti",
            "email": "nathhan.fiscaletti@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "IPStack\\PHP\\": "src/IPStack"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IPStack\\PHP\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "nafisc/extended-arrays": "^1.5",
        "tixastronauta/acc-ip": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*.*",
        "ext-mbstring": "*.*"
    }
}
